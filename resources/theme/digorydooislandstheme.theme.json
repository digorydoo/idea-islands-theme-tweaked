            "nonEditableBackground": "warmGrey33",
            "selectionBackground": "accent"
        },
        "ComboBoxButton": {
            "background": "warmGrey33"
        },
        "Component": {
            "focusColor": "accentDimmed",
            "focusedBorderColor": "accentDimmed"
