        "PopupMenu.Selection.innerInsets": "0,8,0,8",
        "PopupMenu.Selection.outerInsets": "0,0,0,0",
        "PopupMenu.borderInsets": "4,0,4,0",
        "PopupMenu.selectionBackground": "accent",
        "PopupMenuItem.selectionBackground": "accent",
        "SidePanel.background": "warmGrey26",
        "StatusBar.background": "warmGrey29",
        "TabbedPane.background": "warmGrey1f",
